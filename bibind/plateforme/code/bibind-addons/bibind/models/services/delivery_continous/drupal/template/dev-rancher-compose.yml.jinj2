version: '2'
services:
  lb:
    scale: 1
    start_on_create: true
    lb_config:
      certs: []
      port_rules:
      - hostname: {{projetname}}-dev.bibind.com
        priority: 1
        protocol: http
        service: {{projetname}}-dev
    health_check:
      healthy_threshold: 2
      response_timeout: 2000
      port: 42
      unhealthy_threshold: 3
      interval: 2000
  {{projetname}}-db-dev:
    scale: 1
    start_on_create: true
  {{projetname}}-dev:
    scale: 1
    start_on_create: true