		<!-- Cloud Manager service template fournisseur -->
		<record id="view_cloud_service_fournisseur" model="ir.ui.view">
			<field name="name">view.cloud.service.fournisseur.form</field>
			<field name="model">cloud.service.fournisseur</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Cloud Manager service fournisseur" version="7.0">
					<header>
						<button name="ConfigurerService" string="payer le service"
							type="object" />
						<button name="VerifDispo" string="charge log" type="Vérifier la disponibilité" />

						<button name="transfertService" string="Transferer le service"
							type="object" />
						
						<field name="state" widget="statusbar" readonly="1" />

					</header>
